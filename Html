        <img id="image" src="note.png" onClick="imgsrc();" width="1000" height="571">
        <script language="javascript">
            var i = 0;
            var p = document.getElementById("Tbtn");
            var word = ['There is the <br /> some text ', 'There is ', 'There  some text']
            var image = document.getElementById("image");
            var imgs = new Array('note.png', 'note_30.jpg', 'note_30_.png');
            /*   function imgsrc() {
                i++;
                image.src = imgs[i % imgs.length];
            } */
            function next() {
                i++;
                image.src = imgs[i % imgs.length];
                /* p = document.write(word[i % word.length]); */
                p.innerHTML = word[i % word.length];
            }
            function prev() {
                if (i > 0) {
                    i--;
                    image.src = imgs[i % imgs.length];
                    /* p = document.write(word[i % word.length]); */
                    p.innerHTML = word[i % word.length];
                }
                else {
                    i = 2;
                    image.src = imgs[2];
                    /*  p = document.write(word[2]); */
                    p.innerHTML = word[2];
                }
            }
        </script>
    </div>
